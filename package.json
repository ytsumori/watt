{
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "supabase start -x realtime,inbucket,edge-runtime,logflare,vector,pg_prove && prisma generate && pnpm cloud-tasks-up && pnpm db-typegen && next dev",
    "dev/down": "supabase stop && pnpm cloud-tasks-down",
    "build": "prisma generate && prisma migrate deploy && next build && pnpm deploy-cloud-scheduler",
    "preview-build": "prisma generate && next build",
    "start": "next start",
    "test": "vitest",
    "e2e": "playwright test",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "migrate-dev": "dotenv -e .env.local -- prisma migrate dev",
    "create-migration": "dotenv -e .env.local -- prisma migrate dev --create-only",
    "format-schema": "dotenv -e .env.local -- prisma validate & prisma format",
    "cloud-tasks-up": "cd docker && docker compose up -d",
    "cloud-tasks-down": "cd docker && docker compose down",
    "deploy-cloud-scheduler": "pnpm dlx tsx ./scripts/cloudScheduler/deploy.ts",
    "type-check": "tsc --noEmit",
    "db-typegen": "supabase gen types --lang=typescript --local > types/supabase.ts",
    "db-seed": "prisma db seed"
  },
  "prisma": {
    "seed": "pnpm dlx tsx --tsconfig tsconfig.json prisma/seed.ts"
  },
  "dependencies": {
    "@chakra-ui/anatomy": "^2.2.2",
    "@chakra-ui/icons": "^2.1.1",
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@conform-to/react": "^1.1.5",
    "@conform-to/zod": "^1.1.5",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@google-cloud/scheduler": "^4.3.0",
    "@google-cloud/tasks": "4.0.1",
    "@googlemaps/react-wrapper": "^1.1.35",
    "@googlemaps/typescript-guards": "^2.0.3",
    "@grpc/grpc-js": "^1.10.11",
    "@line/bot-sdk": "^8.4.1",
    "@line/liff": "^2.23.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/third-parties": "^14.2.5",
    "@paralleldrive/cuid2": "^2.2.2",
    "@slack/web-api": "^7.3.1",
    "@supabase/ssr": "^0.1.0",
    "@supabase/supabase-js": "^2.44.3",
    "@types/ms": "^0.7.34",
    "@types/node": "20.11.13",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "async-retry": "^1.3.3",
    "date-fns": "^3.6.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "fast-equals": "^5.0.1",
    "framer-motion": "^11.3.2",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "next": "^14.2.5",
    "next-auth": "^4.24.7",
    "next-nprogress-bar": "^2.3.12",
    "prisma": "^5.16.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.13.0",
    "request": "^2.88.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.1",
    "@prisma/client": "^5.16.2",
    "@testing-library/react": "^14.3.1",
    "@types/async-retry": "^1.4.8",
    "@types/google.maps": "^3.55.11",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "jsdom": "^24.1.0",
    "prettier": "3.2.5",
    "supabase": "^1.183.5",
    "vitest": "^1.6.0"
  },
  "packageManager": "pnpm@9.0.4+sha256.caa915eaae9d9aefccf50ee8aeda25a2f8684d8f9d5c6e367eaf176d97c1f89e"
